{{ form_start(form,{'attr': {'role':'form'}}) }}
	<div class="form-group ">
		<label class="control-label no-padding-right">{{ form_label(form.name) }}</label>
		{{ form_widget(form.name,{'attr':{'class':'form-control'}}) }}
		<div class="help-block with-errors"></div>
	</div>

	<div class="form-group ">
		<label class="control-label no-padding-right">{{ form_label(form.address) }}</label>
		{{ form_widget(form.address,{'attr':{'class':'form-control'}}) }}
		<div class="help-block with-errors"></div>
	</div>
<div class="row mb-3">
	<div class="col-md-4 ">
		<label class="control-label no-padding-right">{{ form_label(form.region) }}</label>
		{{ form_widget(form.region,{'attr':{'class':'form-control'}}) }}
		<div class="help-block with-errors"></div>
	</div>
	<div class="col-md-4 ">
		<label class="control-label no-padding-right">{{ form_label(form.zone) }}</label>
		{{ form_widget(form.zone,{'attr':{'class':'form-control'}}) }}
		<div class="help-block with-errors"></div>
	</div>

	<div class="col-md-4">
		<label class="control-label no-padding-right">{{ form_label(form.woreda) }}</label>
		{{ form_widget(form.woreda,{'attr':{'class':'form-control'}}) }}
		<div class="help-block with-errors"></div>
	</div>
    </div>

	<div class="row mb-4">
